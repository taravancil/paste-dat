{
  "name": "dat-gist",
  "version": "1.0.0",
  "description": "A peer-to-peer gist-like app built with Dat",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:static && npm run build:css && npm run build:js",
    "build:js": "browserify lib/index.js -o build/index.js",
    "build:css": "cleancss -o build/main.min.css style/main.css",
    "build:static": "mkdir -p build; cp -r static/* build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taravancil/dat-gist.git"
  },
  "keywords": [
    "dat",
    "gist",
    "beaker browser",
    "peer-to-peer"
  ],
  "author": "Tara Vancil <tbvanc@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taravancil/paste-dat/issues"
  },
  "homepage": "https://github.com/taravancil/past-dat#readme",
  "dependencies": {
    "escape-html": "^1.0.3",
    "marked": "^0.3.6"
  },
  "devDependencies": {
    "browserify": "^14.4.0",
    "clean-css-cli": "^4.1.3"
  }
}
